#import (Config.path +'/Kernel/Autoload/MasterAutoload') = MasterAutoload;

class BodyAutoload extends MasterAutoload {

	confModuleName = 'Body';

	init : function () {
		var templateManager = serviceLocator.get('TemplateManager');
		React['render'](React['createElement'](
			templateManager.get('body_base_main_template').getView(), null),
			document.getElementsByTagName('body')[0]
		);
	}

}

#export BodyAutoload;